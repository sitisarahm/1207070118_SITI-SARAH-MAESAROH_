   "source": [
    "import cv2\n",
    "\n",
    "#load citra\n",
    "img = cv2.imread('bunglon.jpg',0)\n",
    "\n",
    "#aplikasikan laplacian\n",
    "blur = cv2.GaussianBlur(img,(3,3),0)\n",
    "\n",
    "#aplikasikan laplacian\n",
    "laplacian = cv2.Laplacian(blur,cv2.CV_64F)\n",
    "laplacian1 = laplacian/laplacian.max()\n",
    "\n",
    "cv2.imshow('laplacian-gaussian',laplacian1)\n",
    "cv2.waitKey(0)"
   ]
  }
 ],
